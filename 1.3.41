import random

n=int(input())
count1=0.0
count2=0.0
for i in range (1,n+1):
    win=random.randrange(1,4)
    choice=random.randrange(1,4)
    if win==choice:
        count1+=1.0
    else:
        count2+=1.0
print("Probability of success of changing strategy: " + str(count1/n))
print("Probability of success of strategy with no changing: " + str(count2/n))
